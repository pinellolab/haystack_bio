	$(SED) $(SEDSPEC) $? > $@

logging.conf: logging.conf.in 
	$(SED) $(SEDSPEC) $? > $@

%.xsl: %
	$(SED) 's/tname/$?/' xsl-wrapper.in | $(SED) '5r $?' > $@

