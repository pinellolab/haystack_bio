mostlyclean-libtool:
	-rm -f *.lo

clean-libtool:
	-rm -rf .libs _libs
install-memehtmlDATA: $(memehtml_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(memehtmldir)" || $(MKDIR_P) "$(DESTDIR)$(memehtmldir)"
	@list='$(memehtml_DATA)'; test -n "$(memehtmldir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(memehtmldir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(memehtmldir)" || exit $$?; \
	done

uninstall-memehtmlDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(memehtml_DATA)'; test -n "$(memehtmldir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(memehtmldir)'; $(am__uninstall_files_from_dir)
